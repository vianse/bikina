<p id="notice"><%= notice %></p>

<h1>Listing Megusta</h1>

<table>
  <thead>
    <tr>
      <th>Megusta</th>
      <th>User</th>
      <th>Grupo</th>
      <th>Registro</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @megusta.each do |megustum| %>
      <tr>
        <td><%= megustum.megusta %></td>
        <td><%= megustum.user_id %></td>
        <td><%= megustum.grupo_id %></td>
        <td><%= megustum.registro_id %></td>
        <td><%= link_to 'Show', megustum %></td>
        <td><%= link_to 'Edit', edit_megustum_path(megustum) %></td>
        <td><%= link_to 'Destroy', megustum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Megustum', new_megustum_path %>
