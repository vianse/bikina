<p id="notice"><%= notice %></p>

<h1>Listing Cps</h1>

<table>
  <thead>
    <tr>
      <th>Codigo</th>
      <th>Asentamiento</th>
      <th>Tipoasentamiento</th>
      <th>Municipio</th>
      <th>Estado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cps.each do |cp| %>
      <tr>
        <td><%= cp.codigo %></td>
        <td><%= cp.asentamiento %></td>
        <td><%= cp.tipoAsentamiento %></td>
        <td><%= cp.municipio %></td>
        <td><%= cp.estado %></td>
        <td><%= link_to 'Show', cp %></td>
        <td><%= link_to 'Edit', edit_cp_path(cp) %></td>
        <td><%= link_to 'Destroy', cp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cp', new_cp_path %>
