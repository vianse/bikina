              <!-- end page-header -->
               <div class="panel panel-inverse">
                    <div class="panel-body">
                      <div class="row"> 
                        <h2>Creando Perfil</h2>
                           <%= form_for(@perfil) do |f| %>
                            <% if @perfil.errors.any? %>
                              <div id="error_explanation">
                                <h2><%= pluralize(@perfil.errors.count, "error") %> prohibited this perfil from being saved:</h2>

                                <ul>
                                <% @perfil.errors.full_messages.each do |message| %>
                                  <li><%= message %></li>
                                <% end %>
                                </ul>
                              </div>
                            <% end %>

                            <div class="field">
                            <h5>Ingresa tu nombre completo, este aparecera en tus servicios</h5>
                              <%= f.text_field :nombre, :class => 'form-control', :placeholder =>"eje. Francisco Nuñez" %>
                            </div>
                            <h5>Ingresa tu código postal</h5>
                              <%= f.text_field :cp, :class => 'form-control', :placeholder =>"eje. 73993" %>
                            </div>
                            <div class="field">
                              <h5>Agrega una foto de perfil</h5>
                              <%= f.hidden_field :photo %>
                              <%= f.file_field :photo,:class => 'form-control ', :required => true %>
                              <%= f.hidden_field :remote_photo_url %>         
                            </div>
                            <div class="field">
                              <%= f.hidden_field :user_id,:value => current_user.id %>
                            </div>
                            <div class="actions">
                              <%= f.submit "Siguiente" %>
                            </div>
                          <% end %>

                      </div>
                     </div>
               </div>  
